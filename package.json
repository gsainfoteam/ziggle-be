    "passport-http-bearer": "^1.0.1",
    "path": "^0.12.7",
    "prisma": "^5.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.4",
    "ts-case-convert": "^2.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@prisma/client": "^5.3.1",
    "@types/cheerio": "^0.22.35",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
